{
  "name": "@photon-cli/flux",
  "version": "0.3.0",
  "description": "Flux CLI - Connect LangChain agents to iMessage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "flux": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "flux": "bun src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.10.0",
    "better-grpc": "^0.2.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}
